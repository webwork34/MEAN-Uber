{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "google"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "linebreak-style": "off",
    "indent": "off",
    "arrow-parens": "off",
    "new-cap": "off",
    "require-jsdoc": "off",
    "comma-dangle": "off",
    "camelcase": "off",
    "operator-linebreak": "off"
  }
}
